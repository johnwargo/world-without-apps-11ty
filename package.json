{
  "name": "world-without-apps",
  "version": "0.0.6",
  "description": "Static web site for a World Without Apps",
  "author": "John M. Wargo",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnwargo/world-without-apps-11ty"
  },
  "bugs": {
    "url": "https://github.com/johnwargo/world-without-apps-11ty/issues"
  },
  "scripts": {
    "clean": "rimraf _site",
    "build:eleventy": "eleventy",
    "watch:eleventy": "eleventy --serve --quiet",
    "start": "npm-run-all --sequential clean watch:eleventy",
    "build": "npm-run-all --sequential clean build:eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-img": "^3.1.0",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "eleventy-generate-category-pages": "^0.1.4",
    "eleventy-plugin-date": "^1.0.0",
    "eleventy-plugin-youtube-embed": "^1.9.0",
    "generate-build-info": "^0.0.3",
    "html-minifier": "^4.0.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.6",
    "npm-run-all": "^4.1.5",
    "outdent": "^0.8.0",
    "rimraf": "^6.0.1"
  },
  "keywords": [
    "Eleventy"
  ]
}
